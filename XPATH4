from selenium import webdriver
from time import sleep
from selenium.webdriver.common.by import By
driver = webdriver.Chrome()
try:
    driver.get('https://www.lambdatest.com/selenium-playground/table-data-download-demo')
    sleep(2)
    driver.maximize_window()
    input_field = driver.find_element(By.XPATH, '//*[@id="example_wrapper"]/div[1]/a[1]')
    sleep(2)
    input_field.send_keys('example_wrapper')
    sleep(2)
    button = driver.find_element(By.Class, "copy")
    sleep(2)
    button.click()
    sleep(3)
finally:
    driver.quit()
